<h1>Add a Review</h1> 
<% form_for @review do %> 
    <div>
      <%= f.label :rating %>
      <%= f.text_field :rating %>
    </div>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :content %>
      <%= f.text_area :content %>
    </div>
      <%= f.hidden_field :restaurant_id %>  
      <%= f.submit "Create new Review" %> 
    <% end %> 